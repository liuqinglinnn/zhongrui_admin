.weui-tab_content {
  padding-top: 75rpx;
  font-size: 32rpx;
  display: flex;
  flex-direction: column;
}
.weui-tab_content .pageone {
  width: 100%;
  display: flex;
  flex-direction: column;
}
.weui-tab_content .pageone .select {
  width: 100%;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
}
.weui-tab_content .pageone .select .select1 {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  width: 33%;
}
.weui-tab_content .pageone .select .select1 .text1 {
  opacity: 0.7;
  font-size: 30rpx;
}
.weui-tab_content .pageone .select .select2 {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  width: 33%;
}
.weui-tab_content .pageone .select .select2 .text2 {
  opacity: 0.7;
  font-size: 30rpx;
}
.weui-tab_content .pageone .select .select3 {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: center;
  width: 33%;
}
.weui-tab_content .pageone .select .select3 .text3 {
  opacity: 0.7;
  font-size: 30rpx;
}
.weui-tab_content .pageone .search {
  width: 100%;
  height: 100rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}
.weui-tab_content .pageone .search .search_ {
  background-color: white;
  height: 75rpx;
  width: 70%;
  margin-left: 50rpx;
  border-radius: 75rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  box-shadow: 3rpx 3rpx 5rpx 3rpx #c8c3c3;
}
.weui-tab_content .pageone .search .search_ .search_icon {
  width: 50rpx;
  height: 50rpx;
  margin-left: 25rpx;
}
.weui-tab_content .pageone .search .search_ .search_input {
  font-size: 30rpx;
  margin-left: 20rpx;
}
.weui-tab_content .pageone .search .cancel {
  font-size: 30rpx;
  margin-left: 10rpx;
  opacity: 0.7;
  color: black;
}
.weui-tab_content .pageone .carlist {
  width: 100%;
  background-color: #fafafa;
}
.weui-tab_content .pageone .carlist .carlist_item {
  width: 95%;
  margin-left: 2.5%;
  margin-top: 15rpx;
  margin-bottom: 15rpx;
  background-color: white;
  box-shadow: 0px 0px 2px #afafaf;
  border-radius: 10rpx;
  padding-bottom: 10rpx;
}
.weui-tab_content .pageone .carlist .carlist_item_one {
  width: 100%;
  font-size: 35rpx;
  font-weight: bolder;
  text-align: center;
  padding-top: 25rpx;
}
.weui-tab_content .pageone .carlist .carlist_item_state {
  width: 150rpx;
  border-radius: 10rpx;
  height: 50rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  border: 5rpx solid antiquewhite;
  text-align: center;
  position: absolute;
  right: 50rpx;
  margin-top: 100rpx;
  font-size: 25rpx;
  opacity: 0.7;
  font-weight: bold;
}
.weui-tab_content .pageone .carlist .carlist_item_check {
  width: 150rpx;
  border-radius: 15rpx;
  text-align: center;
  position: absolute;
  right: 50rpx;
  margin-top: 200rpx;
  font-size: 25rpx;
  opacity: 0.7;
}
.weui-tab_content .pageone .carlist .carlist_item__ {
  display: flex;
  flex-direction: column;
  justify-content: center;
  width: 100%;
  margin-left: 25rpx;
  margin-top: 25rpx;
}
.weui-tab_content .pageone .carlist .carlist_item__ .carlist_item_two {
  width: 50%;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  opacity: 0.8;
  font-size: 30rpx;
}
.weui-tab_content .pageone .carlist .carlist_item__ .carlist_item_three {
  margin-top: 15rpx;
  width: 50%;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  align-items: center;
  opacity: 0.8;
  font-size: 30rpx;
}
.carlisticon {
  width: 45rpx;
  height: 45rpx;
  display: flex;
  margin-right: 15rpx;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.nav {
  height: 86rpx;
  text-align: center;
  color: #A8A8A8;
  font-size: 4vw;
  background-color: #FFF;
  border-bottom: 1rpx solid #DFDFDF;
  width: 100%;
  box-sizing: border-box;
  overflow: hidden;
  line-height: 80rpx;
  white-space: nowrap;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 99;
}
.nav-item {
  width: 28%;
  display: inline-block;
  text-align: center;
}
.nav-item.active {
  transition: all 0.3s;
  border-bottom: 10rpx solid #478ff3;
  color: #478ff3;
}
.carlist_item_checkbox {
  position: relative;
}
.buttom {
  position: fixed;
  bottom: 0rpx;
  height: 225rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
  background-color: white;
  align-items: center;
  border-top: 2rpx solid #b7b7b7;
}
.buttom .one {
  width: 50%;
  height: auto;
  text-align: center;
  border-left: 2rpx solid #b7b7b7;
  justify-content: center;
  align-items: center;
  display: flex;
  flex-direction: column;
}
.buttom .one .onetext {
  width: 80%;
  height: 75rpx;
  margin: 10rpx;
  border: 5rpx solid #3ab54a;
  background-color: #3ab54a;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 15rpx;
  color: white;
}
.buttom .two {
  width: 50%;
  height: 100rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border-right: 2rpx solid #b7b7b7;
}
.buttom .two .twotext {
  width: 125rpx;
  height: 75rpx;
  margin-left: 15rpx;
  margin-right: 15rpx;
  border: 5rpx solid #3ab54a;
  background-color: #3ab54a;
  display: flex;
  justify-content: center;
  align-items: center;
  border-radius: 15rpx;
  color: white;
}
